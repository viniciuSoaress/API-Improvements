// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Improvements {
  id      String @id @default(uuid())
  name    String
  email   String
  subject String
  message String 
  create_at DateTime @default(now())

  @@map("imporvements")
}
